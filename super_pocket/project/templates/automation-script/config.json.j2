{
  {% if tool_choices.scheduler != "none" %}
  "schedule": "0 * * * *",
  {% endif %}
  {% if features.logging %}
  "log_level": "INFO",
  "log_file": "logs/{{ project_name }}.log",
  {% endif %}
  {% if features.notifications %}
  "notifications": {
    "email": {
      "enabled": false,
      "to": "",
      "from": ""
    },
    "slack": {
      "enabled": false,
      "webhook_url": ""
    }
  },
  {% endif %}
  "tasks": {
    "example_task": {
      "enabled": true
    },
    "data_processing": {
      "enabled": true,
      "batch_size": 100
    }
  }
}
